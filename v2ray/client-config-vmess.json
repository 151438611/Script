// 客户端配置
// 适用于默认走vmess服务器，仅指定规则的 国内域名或IP freedom直连
{
	"log": {
		"loglevel": "warning",
		"access": "/var/log/v2ray_access.log",
		"error": "/var/log/v2ray_error.log"
	},
	"inbounds": [{
		"port": 49452,
		"protocol": "socks",
		"sniffing": {
			"enabled": true,
			"destOverride": ["http", "tls"]
		},
		"settings": {
			"auth": "noauth",
			"udp": false,
			"ip": "127.0.0.1"
		}
	}],
	"outbounds": [{
			"protocol": "vmess",
			"mux": {"enabled": false},
			"settings": {
				"vnext": [
					// 若有多个VPS_Server,按此格式{Server1_config},{Server2_config}...添加即可,实现负载均衡/冗余备份
					{
						"address": "server_ip1",
						"port": 2064,
						"users": [{
							"id": "server_ip1_uuid",
							"security": "auto",
							"alterId": 26 }]
					},
					{
						"address": "server_ip2",
						"port": 1055,
						"users": [{
							"id": "server_ip2_uuid",
							"security": "auto",
							"alterId": 26 }]
					}
				]
			},
			"streamSettings": {
				"network": "tcp"
			}
		},
		{
			"protocol": "freedom",
			"settings": {},
			"tag": "direct_connect"
		},
		{
			"protocol": "blackhole",
			"settings": {},
			"tag": "blackhole_drop"
		}
	],
//	"dns": {
//		"servers": [{
//				"address": "223.5.5.5",
//				"port": 53,
//				"domains": ["geosite:cn"],
//				"expectIPs": ["geoip:cn"]
//			},
//			{
//				"address": "8.8.8.8",
//				"port": 53,
//				"domains": ["geosite:geolocation-!cn"]
//			},
//			"114.114.114.114",
//			"localhost"
//		]
//	},
	"routing": {
		"domainStrategy": "IPOnDemand",
		"rules": [
			{
			"type": "field",
			"outboundTag": "blackhole_drop",
			"domain": [
			"domain:qqq.com",          // 指定域名禁止访问
			"domain:sinaa.com"
				]
			},
			{
				"type": "field",
				"outboundTag": "direct_connect",
				"domain": ["geosite:cn"]
			},
			{
				"type": "field",
				"outboundTag": "direct_connect",
				"ip": ["geoip:cn","geoip:private"]
			}
		]
	}
}